    "email": {
        "enabled": false,
        "email": "example@gmail.com",
        "password": "aaaa bbbb cccc dddd",
        "interval": 7200,
        "delete_after": false
    },

    "entropy": "https://www.random.org/strings/?num=1&len=32&digits=on&upperalpha=on&loweralpha=on&format=plain"
